{
  "name": "dungeon-and-dungeon",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "ts-check": "tsc -b",
    "preview": "vite preview",
    "server:dev": "node --env-file=.env --experimental-strip-types ./src/server/main.ts",
    "server:prod": "NODE_ENV=production node --env-file=.env --experimental-strip-types ./src/server/main.ts",
    "test": "node --test --experimental-strip-types  \"**/*.spec.ts\"",
    "test:coverage": "node --test --experimental-strip-types --experimental-test-coverage  \"**/*.spec.ts\"",
    "test:coverage:report": "node --test --experimental-strip-types --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info \"**/*.spec.ts\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=22.9.0",
    "npm": ">=10.8.3"
  },
  "dependencies": {
    "@auth/express": "^0.8.2",
    "@react-three/drei": "^9.114.3",
    "@react-three/fiber": "^8.17.9",
    "@types/three": "^0.170.0",
    "eventemitter3": "^5.0.1",
    "express": "^4.21.1",
    "next-auth": "^4.24.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.170.0",
    "uuid": "^11.0.0",
    "vite-express": "^0.19.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "3.4.1",
    "storybook": "^8.3.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
